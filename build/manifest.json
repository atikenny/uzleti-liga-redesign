{
    "manifest_version": 2,
    "name": "Uzleti liga redesign",
    "version": "1.2",

    "icons": {
        "16": "img/favicon-16.png",
        "128": "img/favicon-128.png"
    },
    "content_scripts": [
        {
          "matches": ["http://www.uzletiliga.hu/eredmenyek/matches*"],
          "css": ["css/matches.css"],
          "js": ["js/matches.js", "js/jquery.js"],
          "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "font/*.*",
        "img/*.*",
        "js/injected-script.js"
    ]
}
